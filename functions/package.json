{
  "name": "functions",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "babel lib --out-dir dist --delete-dir-on-start --source-maps --ignore ./node_modules",
    "build:watch": "yarn build -- --watch",
    "deploy": "yarn build && firebase deploy --only functions",
    "lint": "eslint .",
    "logs": "firebase functions:log",
    "test": "jest",
    "test:clearCache": "jest --clearCache",
    "serve": "firebase --project=development emulators:start --only auth,firestore,functions,pubsub --import=./seed/data-export --export-on-exit=./seed/data-export",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "start:dev": "NODE_ENV=development concurrently \"yarn\" \"yarn build:watch\" \"yarn serve\"",
    "prettier": "prettier . --write"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.5.0",
    "@qualle-admin/qutil": "^1.54.0",
    "concurrently": "^8.0.1",
    "express": "^4.18.2",
    "firebase": "^9.19.1",
    "firebase-admin": "^11.6.0",
    "firebase-functions": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^27.5.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.24.0",
    "eslint-plugin-es-x": "^5.3.1",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^3.0.0",
    "jest": "^28.1.0"
  },
  "resolutions": {
    "eslint": "8.24.0",
    "firebase-functions": "^4.3.0"
  },
  "private": true
}
