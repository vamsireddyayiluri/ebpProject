{
  "name": "platform-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "^7.1.96",
    "@qualle-admin/qui": "^1.86.0",
    "@qualle-admin/qutil": "latest",
    "@vueuse/core": "^9.9.0",
    "axios": "^1.2.2",
    "firebase": "^9.15.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.40",
    "pinia": "^2.0.29",
    "sass": "^1.57.1",
    "uid": "^2.0.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-simple-password-meter": "^1.1.2",
    "vuetify": "3.0.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.34.0",
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@semantic-release/changelog": "^6.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.6",
    "cypress": "^12.0.2",
    "eslint": "^8.22.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "start-server-and-test": "^1.15.2",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-components": "^0.22.12",
    "unplugin-vue-define-options": "^1.1.1",
    "vite": "^4.0.0",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vuetify": "^1.0.1",
    "vitest": "^0.25.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{vue,js,json,css,scss,md}": "prettier --write"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "semi": false,
    "overrides": [
      {
        "files": [
          "**/*.scss",
          "**/*.html"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog"
    ]
  }
}
